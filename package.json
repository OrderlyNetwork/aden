{
  "name": "orderly-js-sdk-remix-template",
  "description": "Template for Orderly SDK v2 based on Remix",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "clean-reinstall": "rm -rf node_modules yarn.lock package-lock.json && yarn install",
    "build": "remix vite:build",
    "dev": "remix vite:dev",
    "dev:dctailwind": "npx tailwindcss -c ./tailwind.drinkcode.config.js -i ./tailwind.drinkcode.css -o ./app/styles/drinkcode.css --watch",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "preview": "remix-serve ./build/server/index.js",
    "typecheck": "tsc",
    "update-locale-version": "tsx scripts/update-locale-version.ts",
    "merge-locales": "node scripts/merge-locales.cjs"
  },
  "dependencies": {
    "@orderly.network/affiliate": "2.6.4-internal-fix-tpsl-slider.4",
