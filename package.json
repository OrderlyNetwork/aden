    "@orderly.network/trading": "2.6.4-internal-fix-tpsl-slider.4",
    "@orderly.network/trading-leaderboard": "2.6.4-internal-fix-tpsl-slider.4",
    "@orderly.network/trading-rewards": "2.6.4-internal-fix-tpsl-slider.4",
    "@orderly.network/types": "2.6.4-internal-fix-tpsl-slider.4",
    "@orderly.network/ui": "2.6.4-internal-fix-tpsl-slider.4",
    "@orderly.network/ui-chain-selector": "2.6.4-internal-fix-tpsl-slider.4",
    "@orderly.network/ui-scaffold": "2.6.4-internal-fix-tpsl-slider.4",
    "@orderly.network/wallet-connector": "2.6.4-internal-fix-tpsl-slider.4",
    "@orderly.network/wallet-connector-privy": "2.6.4-internal-fix-tpsl-slider.4",
    "@privy-io/cross-app-connect": "^0.2.1",
